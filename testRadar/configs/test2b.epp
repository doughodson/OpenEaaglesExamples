    bgRate: 10

    glutDisplay:
      #include "displays.epp"

    // ---
    // Simulation scenario
    // ---
    ownship: p011
    simulation: ( WorldModel

        numTcThreads: 3
        numBgThreads: 1

        latitude:  REF_LAT
        longitude: REF_LON

