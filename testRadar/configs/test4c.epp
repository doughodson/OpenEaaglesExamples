// ---
// N-squared timing test
//
//    Each player with a RADAR and a RWR (plus one red player with an
//    optional noise jammer), and each player's radar will process
//    all other players as targets (simple n-squared problem). 
//
//    Blue has 3 rows of 8 (24 players) and Red has 3 rows of
//    8 players (24 players), or 48 total players.  To show the doppler
//    shift in the real-beam return, the blue's second and third rows are
//    moving faster, or away from, the first blue row (red shift), and
//    the red players are comming head-on (blue shift).
//
//    Select players on the situation display using the mouse to switch
//    the 'ownship' focus of the radar and rwr displays.
//
//    No track managers, and the R/F parameters are made up.
//    
//    (note: On Intel/Xeon 5140 @ 2.33GHz; with 4 processors), execution time is
//     about 10MS per frame.  Using numTcThreads @ 3 threads, execution time is about
//     6MS per frame)
// ---

#define PFRZ true
//#define JAMMER p135
#define BLUE_HDG 40
#define RED_HDG 220
#define REF_LAT  45.05
#define REF_LON -90.0

#define NUM_BARS 1
#define USE_WORLD true

( TestStation

   tcPriority: 1.0

  bgPriority: 0.5
  bgRate: 10.0


#if 1 /* network test -- replaces test3a */
   netPriority: 1.0
  netRate: 5.0

   networks: {
      ( DisNetIO
         siteID: 1
         applicationID: 3
         exerciseID: 1
         enableInput: true
         enableOutput: true
         enableRelay: false
         netInput:   ( UdpBroadcastHandler localIpAddress: "172.17.160.92"  networkMask: "255.255.248.0" port: 3000  ignoreSourcePort: 3003 shared: true )
         netOutput:  ( UdpBroadcastHandler localIpAddress: "172.17.160.92"  networkMask: "255.255.248.0" port: 3000  localPort: 3003        shared: true )

         // Example emission handler list
         emissionPduHandlers: {
            ( EmissionPduHandler
               emitterName: 777
               emitterFunction: 1
               sensor: ( Radar )
               antenna: ( Antenna
                  polarization: horizontal
                  gain: ( dB 36 )
                  #include "gainPattern.epp"
                  initPosAzimuth:   ( Degrees 0 )
                  initPosElevation: ( Degrees 0 )
                  maxRates: [ 0.8727 0.8727 ]
                  commandRateAzimuth:   ( Degrees 25 ) // per second
                  commandRateElevation: ( Degrees 25 ) // per second
                  reference: [ 0 0 ]
                  searchVolume: [ 1.0472 0.05 ]
                  numBars: 1
                  useWorldCoordinates: true
                  localPlayersOfInterestOnly: true
               )
               defaultIn:  true
               defaultOut:  true
            )
         }

         #include "DisIncomingEntityTypes.epp"
         #include "DisOutgoingEntityTypes.epp"
      )
   }
#endif

